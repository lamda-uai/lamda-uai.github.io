    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.38",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "axios": "^1.4.0",
    "bootstrap": "^5.3.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-bootstrap": "^2.8.0",
    "react-dom": "^17.0.2",
    "react-icons": "^4.10.1",
    "react-paper-list": "^0.4.8",
    "react-router-dom": "^6.14.1",
    "react-scripts": "5.0.1",
    "react-spinners": "^0.13.8",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "resolutions": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "ajv": "^8.0.0",
    "ajv-keywords": "^5.0.0"
  },
  "devDependencies": {
    "@types/bootstrap": "^5",
    "@types/eslint": "^8",
    "@types/gh-pages": "^6",
    "@types/lodash": "^4",
    "@types/prettier": "^2",
    "ajv": "^8.0.0",
    "ajv-keywords": "^5.0.0",
    "eslint": "^8.44.0",
    "gh-pages": "^6.2.0",
    "prettier": "2.8.8"
  },
  "scripts": {
    "start": "REACT_APP_GIT_SHA=$(git rev-parse --short HEAD) REACT_APP_BUILD_TIME=$(date +'%Y-%m-%d %H:%M:%S UTC%z') react-scripts start",
    "build": "REACT_APP_GIT_SHA=$(git rev-parse --short HEAD) REACT_APP_BUILD_TIME=$(date +'%Y-%m-%d %H:%M:%S UTC%z') react-scripts build && bash scripts/gen-legacy-redirect-pages.sh",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "format": "prettier --write .",
    "lint": "eslint --fix ./src/**/*.{js,jsx,ts,tsx}",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "@popperjs/core": "^2.11.0",
    "react-is": "^17.0.2"
  }
}
